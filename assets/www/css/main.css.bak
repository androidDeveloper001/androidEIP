@media screen and (min-width:240px)
{
    .profile-info
   {
     margin-top:35px;
     text-align:center;
     
   }

    .div-profile
    {
     background-image: url('BG.png');
     background-size: cover;
     height: 110px;
     overflow:hidden;
    }

    .no-padding
    {
